{
    "env": {
      "browser": true,
      "es2020": true,
      "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": 0,
      "no-console": 0,
      "consistent-return": ["off"],
      "no-extra-semi": 0
    }
  }
  